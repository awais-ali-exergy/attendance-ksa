  "scripts": {
    "start": "env-cmd -f .env.develop craco  start ",
    "start:prod": "env-cmd -f .env.production craco  start",
    "start:l": "env-cmd -f .env.local craco  start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "lint:fix": "eslint src/**/*.js --fix",
    "build:develop": "env-cmd -f .env.develop npm run-script build",
    "pull": "git pull && yarn && npm run build",
    "push": "git checkout develop && rm -rf build && git reset --hard main && git push --force && yarn && yarn build:develop && git add . && git commit -m 'build' && git push && git checkout main",
    "push:dev": "dev.sh",
    "push:devm": "./dev.sh",
    "push:prod": "./prod.sh",
    "push:prodw": "prod.sh",
    "template": "node ./scripts/template.js",
    "postinstall": "patch-package",
    "get:latest": "latestbuild.sh"
